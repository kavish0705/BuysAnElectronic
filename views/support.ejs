	<div id="title">Support</div>
	<div class="container">
	<form id="support" onsubmit = "return validation()" method="POST" action="/support">
		 <div class="form-row">
    		<div class="col">
    			<label for="fname">First name</label>
      			<input type="text" class="form-control" id="fname" required name="fname">
    		</div>
    		<div class="col">
    			<label for="lname">Last name</label>
      			<input type="text" class="form-control" id="lname" required name="lname">
    		</div>
  		</div>
      <div id="error_message1"></div>

    <div class="form-row">
  		<div class="col">
      			<label id="padd" for="email">Email</label>
      			<input class="form-control" id="email" required name="email">
  		</div>
      <div class="col">
            <label id="padd" for="phone">Phone No.</label>
            <input class="form-control" id="phone" required name="phone">
      </div>
    </div>
    <div id="error_message2"></div>

  		<div class="form-group">
    		<label for="address">Address</label>
    		<input type="text" class="form-control" id="address" placeholder="1234 Main St" required name="address">
  		</div>
      <div id="error_message3"></div>

  		<div class="form-row">
    		<div class="form-group col-md-5">
      			<label for="city">City</label>
      			<input type="text" class="form-control" id="city" required name="city">
   	 		</div>
    		<div class="form-group col-md-4">
      			<label for="state">State</label>
      			<input type="text" class="form-control" id="state" required name="state">
    		</div>
   			 <div class="form-group col-md-3">
      			<label for="postcode">Postcode</label>
      			<input type="text" class="form-control" id="postcode" required name="postcode">
    		</div>
  		</div>
      <div id="error_message4"></div>

  		<div class="form-group">
    		<label for="problem">Description </label>
    		<textarea class="form-control" id="problem" rows="3" required name="tarea"></textarea>
  		</div>
      <div id="error_message5"></div>

  		<label>Is the Product in warranty?</label>  
  		<div>
  			<label for="radio-1">Yes</label>
        <input type="radio" name="radio-1" id="radio-1">
        <label for="radio-2">No</label>
        <input type="radio" name="radio-1" id="radio-2">
		  </div>  		
		<div class="form-group">
    		<label for="exampleFormControlFile1">Attach the original Bill of the product</label>
    		<input type="file" class="form-control-file" id="exampleFormControlFile1">
  		</div>
  		<button type="submit" class="btn btn-primary" aria-required="true">Submit</button>
	</form>
